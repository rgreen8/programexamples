syntax enable
colorscheme slate
set number
set ignorecase
set mouse=a
set title
set scrolloff=3
set clipboard=unnamed
set history=100
set splitright
set noswapfile
set smartindent
set scrolloff=10
set sidescrolloff=15
set sidescroll=1
imap <C-J> <Esc>
set tabstop=4
set softtabstop=0 noexpandtab
set shiftwidth=4
